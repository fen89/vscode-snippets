{
  "name": "vscode-snippets",
  "publisher": "fen89",
  "displayName": "Useful Snippets",
  "description": "Useful Snippets for VS Code",
  "icon": "images/code.png",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/fen89/vscode-snippets"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Angular",
    "Angular 2",
    "Vue",
    "VueJs"
  ],
  "extensionKind": [
    "ui"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/console.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/console.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-angular.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-vue.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-vue-class.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue-html.json"
      }
    ]
  },
  "scripts": {
    "release": "standard-version",
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "standard-version": "^8.0.1"
  }
}
